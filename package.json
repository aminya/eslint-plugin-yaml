{
  "name": "eslint-plugin-yaml",
  "main": "lib/index.js",
  "description": "Lint YAML files",
  "version": "0.4.2",
  "author": "Amin Yahyaabadi, Peter deHaan",
  "scripts": {
    "format": "prettier --write .",
    "test-link": "pnpm link .",
    "test": "eslint --ext yml ./spec ./.github",
    "lint": "eslint .",
    "build": "tsc -p src/tsconfig.json",
    "prepare": "npm run build"
  },
  "files": [
    "src/**",
    "lib/**"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "yaml",
    "yml",
    "eslint-plugin-yaml",
    "eslint-plugin-yml"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aminya/eslint-plugin-yaml.git"
  },
  "bugs": {
    "url": "https://github.com/aminya/eslint-plugin-yaml/issues"
  },
  "homepage": "https://github.com/aminya/eslint-plugin-yaml#readme",
  "directories": {
    "lib": "lib"
  },
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "jshint": "^2.12.0"
  },
  "devDependencies": {
    "@types/eslint": "latest",
    "@types/js-yaml": "^4.0.0",
    "@types/jshint": "^2.12.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-atomic": "^1.12.2",
    "prettier": "latest",
    "typescript": "latest"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 4,
    "printWidth": 120
  }
}
